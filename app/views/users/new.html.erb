<h1>Users#new</h1>
<p>Find me in app/views/users/new.html.erb</p>

<%= form_for @user do |f| %>
    <%= f.label :display_name, "Display Name(visible to other users)" %><br>
    <%= f.text_field :display_name %><br>
    <%= f.label :email %><br>
    <%= f.text_field :email %><br>
    <%= f.label :admin, "Check box if you are a site admin" %><br>
    <%= f.check_box :admin %><br>
    <%= f.label :password %><br>
    <%= f.password_field :password %><br>
    <br>
    <%= f.submit %>
<% end %>